	  </style>
	  <script type="text/javascript">
var $jq = jQuery.noConflict();
$jq(function() {	
	$jq('#divLoading').ajaxStart(function(){
			$jq(this).fadeIn();
			$jq(this).html("<img src='https://siakad.stikesbup.ac.id/images/loading.gif' > ");
		}).ajaxStop(function(){
			$jq(this).fadeOut();
	});
	
	$jq('#formlogin').submit(function(){
	var postData = $jq('#formlogin').serialize();
	var formURL = 'index.php?m=login&p=proses';
		$jq.ajax({
			url : formURL,
			type: "POST",
			data : postData,
			//mimeType:"multipart/form-data",
			dataType: 'html',
			success:function(data) 
			{
				//$jq('.well').slideUp();
				$jq('#divinformasi').html(data);
				$jq('#divinformasi').fadeIn(2000);
			}
		
		});
	return false;
	});
});
</script>  

